<form asp-action="NumaraUret" asp-controller="Numarator" method="get">
    <select class="form-control select2bs4" style="width: 100%;" name="numaratorId" required>
        <option selected disabled value="">Numarator Seçin</option>
        @if (Enumerable.Count(ViewBag.numaratorList) != 0)
            {
                foreach (var item in ViewBag.numaratorList)
                {
                    <option value="@item.NumaratorId">@item.NumaratorAciklama</option>
                }
            }
        </select>
<label>@ViewBag.no</label>
<button type="submit" class="btn ">uret</button>
